{
    "name": "guliano/zf-getid3-module",
    "description": "ZF2 wrapper for the GetID3 library",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2",
        "zend",
        "getid3"
    ],
    "require": {
        "php": ">=5.3.0",
        "zendframework/zend-modulemanager": "~2.0",
        "zendframework/zend-servicemanager": "~2.0",
        "james-heinrich/getid3": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "squizlabs/php_codesniffer": "~2.0",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-4": {
            "ZfGetid3Module\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZfGetid3ModuleTest\\": "test/"
        }
    }
}
